// C Program to check whether you are eligible to drive or not
#include <stdio.h>

int main()
{
    int a;
    printf("Enter your age \n");
    scanf("%d", &a);
    if(a >= 90)
    {
        printf("You are above 90, you cannot drive \n");
    }
    if(a == 50)
    {
        printf("You are Half century\n");
    }
    else{
        printf("You cannot drive\n");
    }
    return 0;
}
